<view class="pintuan">
    <view class="top">
        <text class="bigtitle">拼团专区</text>
        <text class="sorttitle">拼着买更实惠</text>
        <text class="iconfont icon-xianxingtubiaozhizuomoban-07" selectable="false" space="false" decode="false"></text>
    </view>
    <view class="swiperboxbb">
        <swiper class="swiperbb" duration="500">
            <swiper-item class="swiperitembb">
                <scroll-view class="scrollbb" scroll-x="true">
                    <view class="scroollbox">
                        <block wx:for="{{pintuan}}" wx:key="index">
                            <view class="shopitme">
                                <view class="imgbox2">
                                    <image class="" src="{{item.goods_thumb}}"></image>
                                </view>
                                <view class="shopnamebb">{{item.goods_name}}</view>
                                <view class="shoppicebb">
                                    <image class="xiao" src="data:image/png;base64,"></image>
                                    $
                                    <text class="manybb">{{item.team_price}}</text>
                                </view>
                                <view class="delbb">${{item.shop_price}}</view>
                            </view>
                        </block>
                    </view>
                </scroll-view>
            </swiper-item>
        </swiper>
    </view>
</view>